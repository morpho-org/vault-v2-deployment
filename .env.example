# VaultV2 Deployment Environment Variables
# Copy this file to .env and fill in your values

# Role addresses
OWNER=0xYourOwnerAddress
CURATOR=0xYourCuratorAddress
ALLOCATOR=0xYourAllocatorAddress
SENTINEL=0xYourSentinelAddress

# Timelock duration (in seconds) - set to 0 for immediate execution
TIMELOCK_DURATION=0

# VaultV1 address to use as liquidity market
VAULT_V1=0xYourVaultV1Address

# Adapter registry address
ADAPTER_REGISTRY=0xYourAdapterRegistryAddress

# Factory addresses
VAULT_V2_FACTORY=0xYourVaultV2FactoryAddress
MORPHO_VAULT_V1_ADAPTER_FACTORY=0xYourMorphoVaultV1AdapterFactoryAddress

# RPC URL for deployment
RPC_URL=https://your-rpc-url.com

# Private key for deployment (keep this secure!)
PRIVATE_KEY=0xYourPrivateKey

# Optional: Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key
